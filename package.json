      "./packages/TesterApp",
      "./packages/debugger-app",
      "./packages/dashboard",
      "./packages/init",
      "./website"
    ]
  },
  "scripts": {
    "build": "turbo run build",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
